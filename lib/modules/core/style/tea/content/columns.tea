@ global.columns = type_mixin();

.global.oh() {
    float:left;margin:0 2% 2% 0;width:48%;
}

.global.ot() {
    float:left;margin:0 2% 2% 0;width:31.33%;
}

.columns.default() {
    .oh,.ot,.tt,.of,.tf {
        float:left;margin:0 2% 2% 0;width:48%;
    }
    .ot {width:31.33%;}
    .tt {width:64.66%;}
    .of {width:23%;}
    .tf {width:73%;}
    .cl {clear:both;}
}

